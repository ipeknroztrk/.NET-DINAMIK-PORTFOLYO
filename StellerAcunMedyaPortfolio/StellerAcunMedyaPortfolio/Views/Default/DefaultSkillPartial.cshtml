@model List<StellerAcunMedyaPortfolio.Models.TblSkill>

<section class="section">
    <div class="container text-center">
        <h6 class="subtitle">Hakim Olduğum Teknolojiler</h6>
        <h5>Yeteneklerim</h5>
        <p class="mb-5 pb-4">Aşağıda Hakim Olduğum Teknolojilere Göz Atabilirsiniz.  </p>

        
        <div class="row text-left">
            @foreach (var item in Model)
            {
                <div class="col-sm-6">
                    <h6 class="mb-3">@item.SkillName</h6>
                    <div class="progress">
                        <div class="progress-bar bg-primary" role="progressbar" style="width: @item.Value%;" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"><span>@item.Value%</span></div>
                    </div>
                </div>
            }
        </div>
        

    </div>
</section>

